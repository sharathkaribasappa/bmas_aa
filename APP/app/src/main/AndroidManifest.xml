<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.am">

    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_LOGS" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.USE_FINGERPRINT" />
    <uses-permission android:name="android.permission.VIBRATE" />

    <application
        android:name=".BmasApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".activity.RegistrationActivity"
            android:label="@string/title_activity_registration" />
        <activity
            android:name=".activity.LandingActivity"
            android:label="@string/landing" />

        <service android:name=".geofence.GeoFenceIntentService" />

        <receiver
            android:name=".geofence.GeoFenceIntentServiceListener"
            android:exported="false">
            <intent-filter>
                <action android:name="com.am.broadcast_geofenceupdate" />
            </intent-filter>
        </receiver>

        <activity android:name=".activity.LoginLogoutActivity"></activity>
    </application>

</manifest>